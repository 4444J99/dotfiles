{
  "mcpServers": {
    "MCP_DOCKER": {
      "command": "docker",
      "args": [
        "mcp",
        "gateway",
        "run"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "{{ onepasswordRead "op://Personal/master-org-token-110525/token" }}"
      }
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/4444J99/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "desktop-app"
      ]
    }
  },
  "isHardwareAccelerationDisabled": false,
  "preferences": {
    "quickEntryShortcut": "off",
    "localAgentModeTrustedFolders": [
      "/Users/4jp/Library/Mobile Documents/com~apple~CloudDocs/Documents",
      "/Users/4jp/Library/Mobile Documents/com~apple~CloudDocs",
      "/Users/4jp/Workspace/narratological-algorithmic-lenses"
    ],
    "sidebarMode": "task"
  }
}
